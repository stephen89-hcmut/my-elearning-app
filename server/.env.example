# Application
NODE_ENV=development
PORT=3001

# Database (TypeORM)
DB_HOST=localhost
DB_PORT=3306
DB_USERNAME=sManager
DB_PASSWORD=admin@123
DB_NAME=BTL2

# Optional: Prisma connection string (keep in sync with DB_* values above)
# Format: mysql://[user]:[password]@[host]:[port]/[database]
DATABASE_URL="mysql://sManager:admin@123@localhost:3306/BTL2"

# JWT
JWT_SECRET=your_jwt_secret
JWT_EXPIRATION=24h

# CORS
# Comma-separated list of allowed origins
CORS_ORIGIN=http://localhost:5173


if run docker on synology, just use config below

# Application
NODE_ENV=development
PORT=3001

# Database (TypeORM)
DB_HOST=192.168.1.200
DB_PORT=3307
DB_USERNAME=root
DB_PASSWORD=admin@123
DB_NAME=BTL2

# Optional: Prisma connection string (keep in sync with DB_* values above)
# Format: mysql://[user]:[password]@[host]:[port]/[database]
DATABASE_URL="mysql://root:admin@123@192.168.1.200:3307/BTL2"

# JWT
JWT_SECRET=your_jwt_secret_key_here_change_in_production
JWT_EXPIRATION=24h

# CORS
# Comma-separated list of allowed origins
CORS_ORIGIN=http://localhost:5173
